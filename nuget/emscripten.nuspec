<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <!-- The identifier that must be unique within the hosting gallery -->
    <id>emscripten</id>

    <!-- The package version number that is used when resolving dependencies -->
    <version>0.0.0.0</version>

    <!-- Authors contain text that appears directly on the gallery -->
    <authors>axodox</authors>

    <!-- Owners are typically nuget.org identities that allow gallery
         users to earily find other packages by the same owners.  -->
    <owners>axodox</owners>

    <!-- License and project URLs provide links for the gallery -->
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/emscripten-core/emscripten/blob/main/LICENSE</projectUrl>
    <repository type="git" url="https://github.com/emscripten-core/emscripten/blob/main/LICENSE" branch="master"/>

    <!-- The icon is used in Visual Studio's package manager UI -->
    <!--<icon>icon.png</icon>-->

    <!-- If true, this value prompts the user to accept the license when
             installing the package. -->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <!-- Any details about this particular release -->
    <releaseNotes>Initial release</releaseNotes>

    <!-- The description can be used in package manager UI. Note that the
             nuget.org gallery uses information you add in the portal. -->
    <description>Emscripten compiles C and C++ to WebAssembly using LLVM and Binaryen. Emscripten output can run on the Web, in Node.js, and in wasm runtimes.</description>

    <!-- Copyright information -->
    <copyright>Copyright Â©2023 Axodox</copyright>

    <!-- Tags appear in the gallery and can be used for tag searches -->
    <tags>Emscripten</tags>
  </metadata>

  <!-- A readme.txt will be displayed when the package is installed -->
  <files>
    <file src="..\python\3.9.2-nuget_64bit\**\*" target="python" />
    <file src="..\upstream\**\*" target="emcc" />
    <file src="..\node\14.18.2_64bit\**\*" target="node" />
    <file src="..\java\8.152_64bit\**\*" target="java" />

    <file src="emscripten.targets" target="build/native/emscripten.targets" />
    <file src="emscripten.props" target="build/native/emscripten.props" />
    <file src="emscripten-*.xml" target="build/native" />
    <file src=".emscripten" target="emcc/emscripten/.emscripten" />
  </files>
</package>